    "def connexion_to_apis(cve_id):\n",
    "    # CVE API\n",
    "    url = f\"https://cveawg.mitre.org/api/cve/{cve_id}\"\n",
    "    response = req.get(url)\n",
    "\n",
    "    if response.status_code != 200:\n",
    "        #print(f\"Erreur: impossible d'accéder à l'API CVE pour {cve_id}. Statut: {response.status_code}\")\n",
    "        return None\n",
    "    try:\n",
    "        data = response.json()\n",
    "    except req.exceptions.JSONDecodeError:\n",
    "        #print(f\"Erreur: Impossible de décoder la réponse JSON de l'API CVE pour {cve_id}.\")\n",
    "        return None\n",
    "\n",
    "    cve_id = data.get(\"cveMetadata\", {}).get(\"cveId\", None) #On a pas déjà l'id en paramètre ?\n",
    "    cve_info = {}\n",
    "\n",
    "    # Description anglaise\n",
    "    description = None\n",
    "    desc_list = data.get(\"containers\", {}).get(\"cna\", {}).get(\"descriptions\", [])\n",
    "    for d in desc_list:\n",
    "        if d.get(\"lang\") == \"en\":\n",
    "            description = d.get(\"value\")\n",
    "            break\n",
    "\n",
    "    # CVSS score, gestion de plusieurs formats possibles\n",
    "    cvss_score = None\n",
    "    base_severity = None\n",
    "    cvss_score_float = None\n",
    "    metrics = data.get(\"containers\", {}).get(\"cna\", {}).get(\"metrics\", [])\n",
    "    if metrics:\n",
    "        for metric in metrics:\n",
    "            for key in [\"cvssV3_1\", \"cvssV3_0\", \"cvssV2\"]:\n",
    "                if key in metric:\n",
    "                    cvss_score = metric[key].get(\"baseScore\")\n",
    "                    if cvss_score is not None:\n",
    "                        try:\n",
    "                            cvss_score_float = float(cvss_score)\n",
    "                        except (ValueError, TypeError):\n",
    "                            base_severity = \"Inconnu\"\n",
    "\n",
    "                        # Determine Base Severity\n",
    "                        if 9.0 <= cvss_score_float <= 10.0:\n",
    "                            base_severity = \"Critique\"\n",
    "                        elif 7.0 <= cvss_score_float <= 8.0:\n",
    "                            base_severity = \"Élevée\"\n",
    "                        elif 4.0 <= cvss_score_float <= 6.0:\n",
    "                            base_severity = \"Moyenne\"\n",
    "                        elif 0.0 <= cvss_score_float <= 3.0:\n",
    "                            base_severity = \"Faible\"\n",
    "                        else:\n",
    "                            base_severity = \"Aucune\"\n",
    "                        break\n",
    "            if cvss_score is not None:\n",
    "                break\n",
    "    cve_info[\"cvss_score\"] = cvss_score\n",
    "    cve_info[\"base_severity\"] = base_severity\n",
    "\n",
    "    # CWE(s)\n",
    "    cwe_list = []\n",
    "    problem_types = data.get(\"containers\", {}).get(\"cna\", {}).get(\"problemTypes\", [])\n",
    "    for pt in problem_types:\n",
    "        for desc in pt.get(\"descriptions\", []):\n",
    "            if desc.get(\"lang\") == \"en\":\n",
    "                cwe_list.append(desc.get(\"description\"))\n",
    "\n",
    "    cwe = \", \".join(cwe_list) if cwe_list else None\n",
    "\n",
    "    # Produits affectés\n",
    "    affected_products = []\n",
    "    affected = data.get(\"containers\", {}).get(\"cna\", {}).get(\"affected\", [])\n",
    "    for prod in affected:\n",
    "        vendor = prod.get(\"vendor\")\n",
    "        product = prod.get(\"product\")\n",
    "        if vendor and product:\n",
    "            affected_products.append(f\"{vendor} {product}\")\n",
    "\n",
    "    affected_products_str = \", \".join(affected_products) if affected_products else None\n",
    "\n",
    "    # EPSS API\n",
    "    url = f\"https://api.first.org/data/v1/epss?cve={cve_id}\"\n",
    "    response = req.get(url)\n",
    "\n",
    "    if response.status_code != 200:\n",
    "        #print(f\"Erreur: impossible d'accéder à l'API EPSS pour {cve_id}. Statut: {response.status_code}\")\n",
    "        return None\n",
    "    try:\n",
    "        data = response.json()\n",
    "    except req.exceptions.JSONDecodeError:\n",
    "        #print(f\"Erreur: Impossible de décoder la réponse JSON de l'API EPSS pour {cve_id}.\")\n",
    "        return None\n",
    "\n",
    "    infos = data[\"data\"]\n",
    "    epss_score = None\n",
    "    epss_percentile = None\n",
    "    if infos:\n",
    "        epss_info = infos[0]\n",
    "        epss_score = epss_info.get(\"epss\", None)\n",
    "        epss_percentile = epss_info.get(\"percentile\", None)\n",
    "\n",
    "    return {\n",
    "        \"CVE_id\": cve_id,\n",
    "        \"Description\": description,\n",
    "        \"CVSS_score\": cvss_score,\n",
    "        \"Base_severity\": base_severity,\n",
    "        \"CWE\": cwe,\n",
    "        \"Affected_products\": affected_products_str,\n",
    "        \"EPSS_score\": epss_score,\n",
    "        \"EPSS_percentile\": epss_percentile,\n",
    "    }\n"
   ],
